
<h3>User Sign up</h3>
<form action="/users" method="post">
	Email: <input name="email" type="text" value="<%= @user.email %>" required autofocus>
	Password: <input name="password" type="password" required>
	Confirm Password: <input name="password_confirmation" type="password" required>
	<input type="submit" value="Register">
</form>
